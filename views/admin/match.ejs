<% include ../templete/header %>
  <link rel="stylesheet" href="/css/admin/dataTables.bootstrap.min.css">
  <div class="content-wrapper">
   
    <section class="content-header">
      <h1>
        Match  List
       
      </h1>
     
    </section>

    <!-- Main content -->
    <section class="content">
      <!-- Info boxes -->
      <div class="row">
      
      <div class="col-xs-12">
		 <% if(locals.success_messages){ %>
                        <h4 class="msg" style="color: green;margin-left: 10%;"><%=success_messages%></h4>
                        <% } %>
          <div class="box">
            <div class="box-header">
             
			  
            </div>
            <!-- /.box-header -->
            <div class="box-body">
              <table id="example1" class="table table-bordered table-striped">
                <thead>
                <tr>
                  <th>Sr.No</th>
                  <th>UniqueId</th>
                   <th>Team2</th>
                   <th>Team1</th>
                   <th>Type</th>
                   <th>Date</th>
                  
                </tr>
                </thead>
                <tbody>
				<% for(var i=0; i<data.length; i++){ %>
                <tr>
                  <td><%= i+1; %></td>
                  <td><%= data[i].unique_id; %></td>
                  <td><%= data[i].team_2; %></td>
                  <td><%= data[i].team_1; %></td>
                  <td><%= data[i].type; %></td>
                  <td><%= data[i].dateTimeGMT; %></td>
                  
                </tr>
				 <%  } %>


                </tbody>
                
              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>

      </div>
   
    </section>
    <!-- /.content -->
  </div>

  <% include ../templete/footer %>
 
