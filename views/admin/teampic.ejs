<% include ../templete/header %>
  <link rel="stylesheet" href="/css/admin/dataTables.bootstrap.min.css">
  <div class="content-wrapper">
   
    <section class="content-header">
      <h1>
        Team Pic  List
      </h1>
     
    </section>

    <!-- Main content -->
    <section class="content">
      <!-- Info boxes -->
      <div class="row">
      
      <div class="col-xs-12">
		 <% if(locals.success_messages){ %>
                        <h4 class="msg" style="color: green;margin-left: 10%;"><%=success_messages%></h4>
                        <% } %>
          <div class="box">
            <div class="box-header">
             
			  
            </div>
            <!-- /.box-header -->
            <div class="box-body">
              <table id="example1" class="table table-bordered table-striped">
                <thead>
                <tr>
                  <th>Sr.No</th>
                  <th>Team Name</th>
                   <th>Pic</th>
                   <th>Action</th>
                  
                </tr>
                </thead>
                <tbody>
				<% for(var i=0; i<data.length; i++){ %>
                <tr>
                  <td><%= i+1; %></td>
                  <td><%= data[i].name; %></td>
                  <td><img style="width: 75px;float: left;" src="/images/teampic/<%= data[i].pic; %>"></img></td>
                  
                  <td> <a href="/admin/delete_category/<%= data[i]._id; %>" style="font-size: 18px;"><i class="fa fa-edit"></i></a></td>
                  
                </tr>
				 <%  } %>


                </tbody>
                
              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>

      </div>
   
    </section>
    <!-- /.content -->
  </div>

  <% include ../templete/footer %>
 
